{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "userModel",
  "title": "The User Model",
  "type": "object",
  "properties": {
    "id": { "type": "integer" },
    "name": { "type": "string" },
    "email": { "type": "string", "format": "email" },
    "password": { "type": "string", "minLength": 6, "maxLength": 20 }
  }
}
